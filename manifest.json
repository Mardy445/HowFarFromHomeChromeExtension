{
  "manifest_version": 2,
  "name": "How Far From Home?",
  "version": "0.1",
  "key": "",
  "browser_action": {
    "default_icon": "house.png",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "permissions": [
    "activeTab", "tabs", "storage"
  ],
  "content_security_policy": "script-src 'self' https://maps.googleapis.com; object-src 'self'"
}
